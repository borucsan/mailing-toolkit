{
  "name": "@mailing-toolkit/eslint-plugin",
  "version": "0.8.0",
  "description": "Eslint plugin for E-mail templates",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Dawid Karwot",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "package.json"
  ],
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "jest --coverage --silent=false --useStderr",
    "test:watch": "jest --coverage --watchAll --silent=false --useStderr",
    "build": "tsc",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "@html-eslint/eslint-plugin": "^0.24.0",
    "@html-eslint/parser": "^0.24.0",
    "css": "^3.0.0",
    "he": "^1.2.0",
    "inline-style-parser": "^0.2.3",
    "regenerate": "^1.4.2",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@types/css": "^0.0.37",
    "@types/eslint": "^8.56",
    "@types/he": "^1.2.3",
    "@types/jest": "^29.5.12",
    "@types/regenerate": "^1.4.3",
    "eslint": "^8",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "ISC"
}
