    "semver": "^7.5.4",
    "stdio": "^2.1.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-commands": "^2.0.5",
    "@types/command-line-usage": "^5.0.4",
    "@types/node": "^20.11.10",
    "typescript": "^5.3.3"
  },
  "author": "Dawid Karwot <dawidkarwot@gmail.com>",
