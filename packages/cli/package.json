{
  "name": "@mailing-toolkit/cli",
  "version": "0.0.1",
  "description": "CLI tools for creating e-mails templates",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "bin": {
    "mailing-toolkit": "./bin/mailing-toolkit.js"
  },
  "preferGlobal": true,
  "engines": {
