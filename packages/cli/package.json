{
  "name": "@mailing-toolkit/cli",
  "version": "0.8.5",
  "description": "CLI tools for creating e-mails templates",
  "type": "module",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "bin": {
    "mailing-toolkit": "./bin/mailing-toolkit.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "build-ts": "tsc",
    "lint": "eslint --fix && tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "watch-ts": "tsc -w",
    "test:command": "mailing-toolkit",
    "build-install": "npm run build-ts && sudo npm install -g ."
  },
  "dependencies": {
    "@html-eslint/parser": "^0.24.1",
    "@inquirer/prompts": "^5.3.6",
    "@mailing-toolkit/eslint-plugin": "^0.8.1",
    "@types/archiver": "^6.0.2",
    "@types/prompt": "^1.1.8",
    "@web/dev-server": "^0.4.5",
    "archiver": "^7.0.1",
    "axios": "^1.7.2",
    "chokidar": "^3.6.0",
    "colors": "^1.4.0",
    "command-line-args": "^5.2.1",
    "command-line-commands": "^3.0.2",
    "command-line-usage": "^7.0.1",
    "diff": "^5.2.0",
    "env-paths": "^3.0.0",
    "es-html-parser": "^0.0.9",
    "eslint": "^9.9.1",
    "form-data": "^4.0.0",
    "glob": "^10.3.16",
    "html-to-text": "^9.0.5",
    "jimp": "^0.22.10",
    "keypress": "^0.2.1",
    "lodash-es": "^4.17.21",
    "maildev": "^2.1.0",
    "nanoid": "^5.0.7",
    "node-html-parser": "^6.1.12",
    "nodemailer": "^6.9.13",
    "prompt": "^1.3.0",
    "semver": "^7.5.4",
    "stdio": "^2.1.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-commands": "^2.0.5",
    "@types/command-line-usage": "^5.0.4",
    "@types/diff": "^5.2.1",
    "@types/node": "^20.11.10",
    "open": "^10.1.0",
    "typescript": "^5.3.3"
  },
  "author": "Dawid Karwot <dawidkarwot@gmail.com>",
  "license": "MIT"
}
